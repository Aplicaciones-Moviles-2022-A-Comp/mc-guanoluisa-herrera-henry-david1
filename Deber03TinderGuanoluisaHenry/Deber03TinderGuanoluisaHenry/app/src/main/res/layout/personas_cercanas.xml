<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sendaya">

    <TextView
        android:id="@+id/textView_Universidad"
        android:layout_width="131dp"
        android:layout_height="35dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="2dp"
        android:background="#4A121111"
        android:fontFamily="sans-serif-medium"
        android:paddingLeft="18px"
        android:paddingTop="18px"
        android:text="TextView"
        android:textColor="#FFFFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_Edad" />

    <ImageButton
        android:id="@+id/imageButton_tinderPlus"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="12dp"
        android:backgroundTint="#00F6F6F6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ellipse_1" />

    <ImageButton
        android:id="@+id/imageButton_NoLike"
        android:layout_width="64dp"
        android:layout_height="61dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="6dp"
        android:background="#00FDFDFD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton_tinderPlus"
        app:srcCompat="@drawable/ellipse_2" />

    <ImageButton
        android:id="@+id/imageButton_SuperLike"
        android:layout_width="51dp"
        android:layout_height="61dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton_NoLike"
        app:srcCompat="@drawable/ellipse_4" />

    <ImageButton
        android:id="@+id/imageButton_Like"
        android:layout_width="69dp"
        android:layout_height="61dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="6dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton_SuperLike"
        app:srcCompat="@drawable/ellipse_3" />

    <ImageButton
        android:id="@+id/imageButton_Trueno"
        android:layout_width="51dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="12dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageButton_Like"
        app:srcCompat="@drawable/ellipse_5" />

    <TextView
        android:id="@+id/textView_Nombre"
        android:layout_width="131dp"
        android:layout_height="35dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="476dp"
        android:background="#4A121111"
        android:fontFamily="sans-serif-medium"
        android:paddingLeft="18px"
        android:paddingTop="18px"
        android:text="TextView"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_Edad"
        android:layout_width="131dp"
        android:layout_height="35dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="2dp"
        android:background="#4A121111"
        android:fontFamily="sans-serif-medium"
        android:paddingLeft="18px"
        android:paddingTop="18px"
        android:text="TextView"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_Nombre" />

    <TextView
        android:id="@+id/textView_Likes"
        android:layout_width="43dp"
        android:layout_height="29dp"
        android:layout_marginTop="60dp"
        android:background="#90121111"
        android:text="0"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.956"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>